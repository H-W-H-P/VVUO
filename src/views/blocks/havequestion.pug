.have_question.max_width
    h2.have_question__title Есть вопросы? Проконсультируем
    form(action="")
        .have_question__field_input
            label(for="").have_question__input.danger.input_decore_label
                input(type="text", placeholder="").input_decore
                span.label_text Ваше имя
                span.border
                span.danger_text Неверное имя
            label(for="").have_question__input.input_decore_label
                input(type="tel", placeholder="").input_decore
                span.label_text Телефон
                span.border
                span.danger_text Неверный номер телефона
            label(for="").have_question__input.input_decore_label.have_question__select_wrap
                //- input(type="text", placeholder="").input_decore
                //- span.label_text Время для звонка
                //- span.border
                select#time(name="").have_question__select
                    option(value="default" disabled selected) Время для звонка
                    option(value="1") 12:00
                    option(value="1") 14:00
                    option(value="1") 16:00
            .shop_filters__block.have_question__input.have_question__select_new
                a(href="#").shop_filters__cat.bold.css_arr.closed.conf_wr__cat Время для звонка
                    .conf_wr_filters__cat_wr.shop_filters__cat_wr.closed
                        .shop_filters__input_wr.conf_wr_filters__input_wr
                            input(type="checkbox").checkbox#input1
                            label(for="input5").conf_wr_filters-side__chbx.label_checkbox(data-type="chair") 12:00
                        .shop_filters__input_wr.conf_wr_filters__input_wr
                            input(type="checkbox").checkbox#input2
                            label(for="input6").conf_wr_filters-side__chbx.label_checkbox(data-type="table") 14:00
                        .shop_filters__input_wr.conf_wr_filters__input_wr
                            input(type="checkbox").checkbox#input3
                            label(for="input7").conf_wr_filters-side__chbx.label_checkbox 16:00
        .have_question__field_send
            button.have_question__btn.bold Отправить заявку
                .hover_bl
            p.have_question__consalt_free Консультация бесплатно
